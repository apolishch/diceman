{
  "name": "node-server-skeleton",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "git@github.com:flockcover/node-server-skeleton.git",
  "author": "<cr.mansel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "gulp && gulp buildSrc",
